<div class="form">
  <h2>Make your changes!</h2>
  <h1>🍸</h1>
  <%= simple_form_for [@cocktail, @dose] do |f| %>
    <%= f.input :description %>
    <%= f.association :ingredient %>
    <%= f.button :submit, class:"btn btn-warning" %>
  <% end %>
  <%= link_to "Nvm, let's go back", cocktail_path(@cocktail) %>
</div>